        "# --- Step 3: Define the Chatbot Logic Function ---\n",
        "# This function will process user input and manage chat history.\n",
        "def respond(message, history):\n",
        "    # Convert Gradio's history format to LangChain's format\n",
        "    chat_history = ChatMessageHistory()\n",
        "    for human, ai in history:\n",
        "        chat_history.add_user_message(human)\n",
        "        chat_history.add_ai_message(ai)\n",
        "\n",
        "    # Invoke the agent to get a response\n",
        "    response = agent_executor.invoke({\n",
        "        \"input\": message,\n",
        "        \"chat_history\": chat_history.messages\n",
        "    })\n",
        "\n",
        "    # Return the agent's output to be displayed in the UI\n",
        "    return response[\"output\"]\n",
